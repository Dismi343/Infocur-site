{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://infocuor-backend-latest.onrender.com/api/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "https://infocuor-backend-latest.onrender.com/auth/:path*"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
